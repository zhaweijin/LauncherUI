<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/widget28"
    android:layout_width="wrap_content"
    android:layout_height="fill_parent"
    android:background="@color/all_background"
    android:orientation="horizontal" >

    <LinearLayout
        android:id="@+id/widget31"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent" >

        <LinearLayout
            android:id="@+id/left"
            android:layout_width="440dip"
            android:layout_height="fill_parent"
            android:orientation="horizontal" >

            <RelativeLayout
                android:id="@+id/widget31"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent" >

                <ImageView
                    android:id="@+id/setting_icon"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="120dip"
                    android:background="@drawable/setting_icon" >
                </ImageView>
                
                <TextView
                    android:id="@+id/setting_subtitle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/setting_icon"
                    android:layout_centerHorizontal="true"
                    android:textColor="@android:color/white"
                    android:textSize="30sp"
                    android:layout_marginTop="30dip"
                    >
                </TextView>
 
                 <Button
                    android:id="@+id/open_storage_sdcard"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/open_storage_internal"
                    android:background="@drawable/button_background"
                    android:layout_alignParentLeft="true"
                    android:visibility="gone"
                    android:textColor="@android:color/white"
                    android:nextFocusRight="@+id/layout_network"
                    android:text="@string/open_storage_sdcard" >
                </Button>
                <Button
                    android:id="@+id/open_storage_internal"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/more_setting"
                    android:background="@drawable/button_background"
                    android:layout_alignParentLeft="true"
                    android:visibility="gone"
                    android:textColor="@android:color/white"
                    android:nextFocusRight="@+id/layout_network"
                    android:text="@string/open_storage_internal" >
                </Button>
                <Button
                    android:id="@+id/open_or_close_wifi"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/more_setting"
                    android:background="@drawable/button_background"
                    android:layout_alignParentLeft="true"
                    android:visibility="gone"
                    android:textColor="@android:color/white"
                    android:nextFocusRight="@+id/layout_network"
                    android:text="@string/open_wifi" >
                </Button>
                <Button
                    android:id="@+id/more_setting"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentLeft="true"
                    android:textColor="@android:color/white"
                    android:background="@drawable/button_background"
                    android:nextFocusRight="@+id/layout_network"
                    android:text="@string/more_setting" >
                </Button>
            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/widget30"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:background="@drawable/market_vertical_dot"
            android:orientation="vertical" >
        </LinearLayout>

        <LinearLayout
            android:id="@+id/right"
            android:layout_width="840dip"
            android:layout_height="fill_parent"
            android:orientation="vertical" >

            <LinearLayout
                android:id="@+id/widget34"
                android:layout_width="fill_parent"
                android:layout_height="100dip"
                android:orientation="horizontal" >

                <RelativeLayout
                    android:id="@+id/layout_network"
                    android:layout_width="139dip"
                    android:layout_height="fill_parent"
                    android:background="@drawable/setting_right_top_button_select"
                    android:focusable="true"
                    android:orientation="vertical" >

                    <ImageView
                        android:id="@+id/network"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:background="@drawable/setting_network" >
                    </ImageView>
                </RelativeLayout>

                <LinearLayout
                    android:id="@+id/widget30"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:background="@drawable/market_vertical_dot"
                    android:orientation="vertical" >
                </LinearLayout>

                <RelativeLayout
                    android:id="@+id/layout_display"
                    android:layout_width="139dip"
                    android:layout_height="fill_parent"
                    android:background="@drawable/setting_right_top_button_select"
                    android:focusable="true"
                    android:orientation="vertical" >

                    <ImageView
                        android:id="@+id/display"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:background="@drawable/setting_display" >
                    </ImageView>
                </RelativeLayout>

                <LinearLayout
                    android:id="@+id/widget30"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:background="@drawable/market_vertical_dot"
                    android:orientation="vertical" >
                </LinearLayout>

                <RelativeLayout
                    android:id="@+id/layout_volume"
                    android:layout_width="139dip"
                    android:layout_height="fill_parent"
                    android:background="@drawable/setting_right_top_button_select"
                    android:focusable="true" >

                    <ImageView
                        android:id="@+id/volume"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:background="@drawable/setting_volume" >
                    </ImageView>
                </RelativeLayout>

                <LinearLayout
                    android:id="@+id/widget30"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:background="@drawable/market_vertical_dot"
                    android:orientation="vertical" >
                </LinearLayout>

                <RelativeLayout
                    android:id="@+id/layout_storage"
                    android:layout_width="139dip"
                    android:layout_height="fill_parent"
                    android:background="@drawable/setting_right_top_button_select"
                    android:focusable="true"
                    android:orientation="vertical" >

                    <ImageView
                        android:id="@+id/storage"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:background="@drawable/setting_storage" >
                    </ImageView>
                </RelativeLayout>

                <LinearLayout
                    android:id="@+id/widget30"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:background="@drawable/market_vertical_dot"
                    android:orientation="vertical" >
                </LinearLayout>

                <RelativeLayout
                    android:id="@+id/layout_security"
                    android:layout_width="139dip"
                    android:layout_height="fill_parent"
                    android:background="@drawable/setting_right_top_button_select"
                    android:focusable="true"
                    android:orientation="vertical" >

                    <ImageView
                        android:id="@+id/security"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:background="@drawable/setting_security" >
                    </ImageView>
                </RelativeLayout>

                <LinearLayout
                    android:id="@+id/widget30"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:background="@drawable/market_vertical_dot"
                    android:orientation="vertical" >
                </LinearLayout>

                <RelativeLayout
                    android:id="@+id/layout_update"
                    android:layout_width="139dip"
                    android:layout_height="fill_parent"
                    android:background="@drawable/setting_right_top_button_select"
                    android:focusable="true"
                    android:orientation="vertical" >

                    <ImageView
                        android:id="@+id/update"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:background="@drawable/setting_update" >
                    </ImageView>
                </RelativeLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/horizontal_div"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/setting_horizontal_div"
                android:orientation="vertical" >
            </LinearLayout>

            <LinearLayout
                android:id="@+id/containerBody"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:measureAllChildren="true"
                android:orientation="vertical" >
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

</LinearLayout>